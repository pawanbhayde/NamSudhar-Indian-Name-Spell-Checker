{
  "manifest_version": 2,
  "name": "Spell Checker Extension",
  "version": "1.0",
  "description": "A Chrome extension for spell checking Indian names",
  "browser_action": {
    "default_popup": "index.html"
    
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
